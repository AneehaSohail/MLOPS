import numpy as np

# Read documents (each line is a document)
with open("documents.txt", "r", encoding="utf-8") as f:
    docs = [line.strip() for line in f if line.strip()]

# Create numeric embeddings: (num_docs x 50)
embeddings = np.random.rand(len(docs), 50).astype(np.float32)

np.save("embeddings.npy", embeddings)
print("saved", embeddings.shape, embeddings.dtype)
